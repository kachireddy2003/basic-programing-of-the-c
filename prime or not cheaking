//if(n%i==0)
//count =count=1
//count==2(then print)
#include<stdio.h>
int main()
{
    int i,n,count=0;
    n=4;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        count=count+1;
    }
    if(count==2)
    printf("the given nuber is prime number%d",n);
    else
    printf("the given number is not prime%d",n);
    
}
